<section class="section jujuy-post">
    <ul class="jujuy-post-list has-scrollbar">
        <% for (let i = 1; i < 9; i++) { %>
            <li class="jujuy-post-item">
                <img src="images/home/post-list/insta-<%= i %>.jpg" width="100" height="100" loading="lazy" alt="Imagen Jujuy <%= i %>"
                class="jujuy-post-banner image-contain">

                <a href="#" class="jujuy-post-link">
                    <ion-icon name="logo-instagram"></ion-icon>
                </a>
            </li>
      <% } %>
    </ul>
</section>
